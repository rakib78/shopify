// popup style
#popwrap {
  display:none;
  background-color: rgba(0,0,0,0.8);
  position: fixed;
  height: 100%;
  width: 100%;
  z-index: 9999999;
  overflow-y: auto;
}
  
#popwrap .popcontent-wrap {
  display: block;
  margin: 0 auto;
  padding: 0;
  position: relative;
  max-width: 800px;
  display: table;
  height: 100%;
} 
.img-wrap{
  display:inline-block;
  position: relative;
  padding: 5px 10px;
  background-color: #fff;
  box-sizing: border-box;
}
  
#pop-close{
  position: absolute;
  right: -1px;
  top: -7px;
  font-size:30px;
  cursor: pointer;
  color: #00aedb;
}
#pop-open{
  cursor: pointer;
}

.pop-content{
  display: table-row;
}
.pop-content a{
  display: table-cell;
  vertical-align: middle;
}  

#popwrap img.mob-ver{
    display:none;
}

body.no-scrl{
  overflow-y: hidden;
}
  

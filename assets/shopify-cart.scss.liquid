body.ext-product-popup-active{
  overflow:hidden;
}
#ext-product-popup-wrap {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    z-index: 9999;
    background: rgba(0,0,0,0.8);
    display: none;
    padding:10px; 
}
#ext-product-popup-wrap .ext-product-popup {
    position: relative;
    max-width: 460px;
    margin: 0 auto;
    padding: 20px 0;
    background-color: #fff;
    text-align: center;
    border: 2px solid #00aedb;
    border-radius: 15px;
    overflow:hidden;
    
}
.ext-product-popup {
}

#purchase-diffuser-modal .close {
  z-index: 9999;
  padding: 0;
  right: 10px;
  top: -5px;
}

#ext-product {
  overflow-y: auto;
  overflow-x: hidden;
  font-family: 'AdelleSans', arial;

  .ck-url {
    padding: 5px 45px 2px;
  }
  .img-responsive {
    width: 70%;
    margin: auto;

    @media screen and (max-width: 375px) {
      width: 100%;
    }

    .variant-image {
      margin-top: -65px;
      margin-bottom: -55px;

      @media screen and (max-width: 375px) {
        margin-top: -50px;
      }
    }
  }
}
#ext-product h1, #ext-product h2, #ext-product p, #ext-product a, #ext-product li{
    font-family: 'AdelleSans', arial;
}
.ext-product-popup-close {
    position: absolute;
    background: #00D85F;
    color: #fff;
    width: 40px;
    height: 40px;
    line-height: 40px;
    font-weight: bold;
    right: 0;
    top: 0;
    font-family: arial;
    font-size: 18px;
    cursor: pointer;
}
.ext-product-title .variant-title{
	display:none;
}

.ext-product-title h1.product-title {
    margin-bottom: 10px;
    padding-bottom: 0;
    color: #00aedb;
}
.ext-procuct-price {
    padding-bottom: 10px;
}
.ext-procuct-price .none{
  display:none;
}
.ext-procuct-price span {
    display: inline-block;
    font-size: 22px;
    padding: 0 8px;
}
.ext-procuct-price .variant-compareAtPrice {
    position: relative;
    color: #00aedb;
}
.ext-procuct-price .variant-compareAtPrice:before {
    content: "";
    width: 84%;
    height: 1px;
    background: #00aedb;
    position: absolute;
    top: 50%;
}
.ext-procuct-price .variant-price {
    color: red;
}

.extvariant-selectors ul {
    padding: 0;
    margin: 0;
    display: block;
    padding-bottom: 10px;
}
.extvariant-selectors ul li {
    display: inline-block;
    border: 1px solid #e2e2e2;
    border-radius: 20px;
    color:#424242;
    cursor: pointer;
    display: inline-block;
    font-size: 15px;
    font-weight: 600;
    list-style: outside none none;
    padding: 8px 15px 5px;
    text-transform: uppercase; 
    margin: 5px;

    @media screen and (max-width: 768px) {
      margin: 10px 3px 2px;
    }
}
.extvariant-selectors ul li.v-active{
	background: #01addb;
  color: #fff; 
}

